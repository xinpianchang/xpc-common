{
  "name": "@newstudios/common",
  "version": "0.2.0",
  "description": "a common utils for js",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build:js": "babel src --no-comments --extensions \".ts,.tsx\" --out-dir dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "concurrently \"npm:build:*\"",
    "pre:types": "tsc --build tsconfig.prod.json",
    "pre:js": "babel . --ignore \"src,node_modules\" --no-comments --extensions .ts -d .",
    "pre": "concurrently \"npm:pre:*\"",
    "prepublishOnly": "npm run clean:build",
    "clean": "rimraf dist && rimraf ./*.d.ts && rimraf ./*.js",
    "clean:build": "npm run clean && npm run build && npm run pre",
    "lint": "eslint src/**/* --fix",
    "types:check": "tsc --noEmit",
    "types:watch": "npm run types:check -- --watch",
    "start": "concurrently \"npm run build:js -- --watch\" \"npm run build:types -- -w\"",
    "dryrun": "npm publish --access public --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinpianchang/xpc-common.git"
  },
  "keywords": [
    "javascript",
    "common",
    "utils"
  ],
  "author": "tangye <tangye@xinpianchang.com>",
  "license": "MIT",
  "homepage": "https://github.com/xinpianchang/xpc-common#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/plugin-transform-typescript": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.2.0",
    "lint-staged": "^10.2.9",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "files": [
    "dist",
    "index.js",
    "index.d.ts",
    "errors.js",
    "errors.d.ts",
    "event.js",
    "event.d.ts",
    "functional.js",
    "functional.d.ts",
    "iterator.js",
    "iterator.d.ts",
    "lifecycle.js",
    "lifecycle.d.ts",
    "linkedList.js",
    "linkedList.d.ts",
    "async.js",
    "async.d.ts",
    "cancellation.js",
    "cancellation.d.ts"
  ]
}
